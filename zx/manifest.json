{
    "author": "Dev. LR",
    "name": "Gemini Research Summarizer",
    "description": "Summarize research papers with detailed analysis using Gemini AI.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup_menu.html"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
      }
    ]
  }
  